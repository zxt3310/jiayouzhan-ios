<?xml version="1.0" encoding="UTF-8"?>
<FlexScrollView layout="flex:1" attr="vertScroll:true,bgColor:#f2f7f8">
    <FlexContainerView name="_cellView" layout="" attr="">
        <!-- <REAccesoryCell attr=""/> -->
    </FlexContainerView>
    <FlexTouchView name="_addBtn" onPress="addCell" layout="height:50,justifyContent:center,alignItems:center" attr="">
        <UILabel name="" layout="" attr="fontSize:14,color:#1c713d,text:+ 添加配件选项"/>
    </FlexTouchView>
    <UIView name="_submitView" layout="height:200,justifyContent:center,alignItems:center" attr="">
        <FlexTouchView onPress="submit" layout="width:200,height:50,justifyContent:center,alignItems:center" attr="bgColor:#1c713d">
            <UILabel name="" layout="" attr="fontSize:14,color:white,lineBreakMode:truncatingTail,linesNum:1,text:提交"/>
        </FlexTouchView>
    </UIView>
    
    <FlexModalView name="modal" layout="width:100%,height:60%" attr="bgColor:#f5f5f5,position:bottom,cancelable:true">
        <REAccPicker name="_accPicker" layout="flex:1" attr=""/>
    </FlexModalView>
   
</FlexScrollView>

